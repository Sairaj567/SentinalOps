[sentinel]
sentinel-server ansible_host=10.0.1.10 ansible_user=ubuntu

[victims]
victim-server ansible_host=10.0.2.10 ansible_user=ubuntu

[attackers]
attacker-machine ansible_host=10.0.2.20 ansible_user=kali

[agents:children]
victims

[all:vars]
ansible_python_interpreter=/usr/bin/python3
ansible_ssh_private_key_file=~/.ssh/sentinelops-key.pem
